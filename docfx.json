{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/FrameworkExceptions.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "src": "../FS/"
        }
      ],
      "dest": "obj/api",
      "filter": "api-filter-config.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "src": "obj"
      },
      {
        "files": [
          "doc/**.md",
          "doc/**.yml"
        ]
      },
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ]
  }
}