{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/FrameworkExceptions.csproj",
            "**/FrameworkDataProvider.csproj",
            "**/FrameworkBase.csproj",
            "**/FrameworkStudio.DevFormBase.csproj",
            "**/FrameworkControls.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "src": "../fsdocs-fs/"
        }
      ],
      "dest": "obj/api",
      "filter": "api-filter-config.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "src": "obj"
      },
      {
        "files": [
          "articles/**.md",
          "articles/**.yml"
        ]
      },
      {
        "files": [
          "doc/**.md",
          "doc/**.yml"
        ]
      },
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default",
      "templates/memberpage.2.45.0/content",
      "templates/frameworkstudio/content"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ]
  }
}